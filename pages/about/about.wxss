.me-head {
  padding: 30rpx 30rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  box-sizing: border-box;
}

.me-head .name {
  color: #333;
  font-size: 36rpx;
}

.me-head .avatar {
  height: 96rpx;
  width: 96rpx;
}

.me-item {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  padding: 0rpx 30rpx;
  height: 80rpx;
  align-items: center;
  align-self: left;
  width: 100%;
}

.me-item .icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 40rpx;
}

.line {
  width: 92%;
  height: 2rpx;
  background-color: #efefef
}

.text-second {
  font-size: 28rpx;
  color: #999;
}

.project-title {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 42rpx;
  align-self: flex-start;
  margin-top: 30rpx;
}

.project-content {
  font-size: 28rpx;
  color: #999;
  align-self: flex-start;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.project-url {
  font-size:28rpx;
  color: #666;
  align-self: flex-start;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 8rpx;
  margin-bottom: 8rpx;
}